
<%= simple_form_for(@project) do |f| %>
    <%= f.input :title, placeholder: "What is your project about?" %>
    <%= f.input :description, placeholder: "Add a description to your project...", as: :text %>
    <%= f.input :start_time, placeholder: "Start Time" %>
    <%= f.input :end_time, placeholder: "End Time" %>
    <%= f.input :location, placeholder: "Where?" %>
    <%= f.input :picture %>
    <%= f.input :picture_cache, as: :hidden %>

    <%= f.button :submit, class: "btn btn-primary"  %>
<% end %>




